<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.multidatasource.aop.mapper.StudentMapper">
    <resultMap id="stuResultMap" type="com.demo.multidatasource.aop.entity.Student">
        <id column="id"  property="id"></id>
        <result column="name"  property="name"></result>
        <result column="addr"  property="addr"></result>
    </resultMap>

    <select id="selectAllStudents" resultMap="stuResultMap">
        select id, name, addr from student;
    </select>
</mapper>
