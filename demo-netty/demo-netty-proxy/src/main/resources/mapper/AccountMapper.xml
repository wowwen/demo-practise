<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.netty.proxy.mapper.AccountMapper">


    <select id="selectByName" resultType="com.demo.netty.proxy.model.ServiceAccount">
        select sa.id, sa.username, sa.password from service_account sa
        <where>
            sa.username = #{username}
        </where>
    </select>
</mapper>